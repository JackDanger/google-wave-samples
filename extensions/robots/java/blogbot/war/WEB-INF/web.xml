<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
  <servlet>
    <servlet-name>Blogbot</servlet-name>
    <servlet-class>com.google.wave.extensions.blogbot.BlogbotServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Blogbot</servlet-name>
    <url-pattern>/_wave/robot/jsonrpc</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BlogbotProfileServlet</servlet-name>
    <servlet-class>com.google.wave.extensions.blogbot.BlogbotProfileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BlogbotProfileServlet</servlet-name>
    <url-pattern>/_wave/robot/profile</url-pattern>
  </servlet-mapping>
</web-app>
